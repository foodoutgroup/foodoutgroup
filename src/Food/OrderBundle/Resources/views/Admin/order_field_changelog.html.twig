<td colspan="2">
    <b>{{ field_description.options.label|trans({}, 'SonataAdminBundle') }}</b>
    <br/>
    <div class="collapsable">
        <h4 class="padded">{{ 'admin.order.data_preview'|trans({}, 'SonataAdminBundle') }}</h4>
        <div id="field_container_details" class="field-container">
            <table>
                <tr>
                    <th>{{ 'admin.order.date'|trans({}, 'SonataAdminBundle') }}</th>
                    <th>{{ 'admin.order.fieldname'|trans({}, 'SonataAdminBundle') }}</th>
                    <th>{{ 'admin.order.old_value'|trans({}, 'SonataAdminBundle') }}</th>
                    <th>{{ 'admin.order.new_value'|trans({}, 'SonataAdminBundle') }}</th>
                    <th>{{ 'admin.order.username'|trans({}, 'SonataAdminBundle') }}</th>
                </tr>
            {% for fieldInfo in object.orderFieldChangelog %}
                <tr>
                    <td>{{ fieldInfo.date|date("Y-m-d H:i:s") }}</td>
                    <td>{{ fieldInfo.fieldname }}</td>
                    <td>{{ fieldInfo.oldValue }}</td>
                    <td>{{ fieldInfo.newValue }}</td>
                    <td>{{ fieldInfo.user }}</td>
                </tr>
            {% endfor %}
            </table>
        </div>
    </div>
</td>