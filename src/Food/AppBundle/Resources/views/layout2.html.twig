<body>
{{ notifications.init|raw }}
<div class="site">
    <div class="site-top">
        {#<div class="site-block clearfix"><a href="{{ path('food_lang_homepage') }}" class="main-logo"><img alt="{{ page_title }}" src="{{ asset('bundles/foodapp/images/logo_food_easter_lt.png') }}"></a>#}
        <div class="site-block clearfix"><a href="{{ path('food_lang_homepage') }}" class="main-logo"><img alt="{{ page_title }}" src="{{ asset('bundles/foodapp/images/logo_food.png') }}"></a>

                {# <p class="contact-phone">1822 <span>užsakymas telefonu</span></p> #}
            <ul class="menu top-menu clearfix" data-toggle="popover" data-placement="bottom" data-content="" data-position-fix="840">
                <li><a href="{{ path('food_slug', {'slug': ('general.help_slug'|trans())}) }}"><span>{% trans %}general.help_button{% endtrans %}</span></a></li>
                <li class="login-ico">
                    {% render(controller('FoodUserBundle:Default:loginButton')) %}
                </li>
            </ul>
        </div>
    </div>

    {% block body %}{% endblock %}
    <div class="site-center site-center-bottom">
        <div class="site-block">
            <div class="clearfix bottom-blocks">
                <div class="app-block clearfix"><img src="//{{ cloudfront_url }}{{ asset('bundles/foodapp/images/app-view.png') }}">
                    <div class="app-block-inner">
                        <h3>Foodout.lt tavo kišenėje!</h3>
                        <p>Užsisakyk maisto į namus ar biurą per iPhone arba Android telefonus. Atsisiųsk nemokamą aplikaciją!.</p>
                        <div><a target="_BLANK" href="https://itunes.apple.com/us/app/foodout/id919992168"><img src="{{ asset('bundles/foodapp/images/app-store.png') }}"></a><a target="_BLANK" href="https://play.google.com/store/apps/details?id=co.lemonlabs.android.foodout#"><img src="{{ asset('bundles/foodapp/images/g-store.png') }}"></a></div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    {{ render(controller('FoodAppBundle:Default:footer')) }}
</div>

{{ include('FoodAppBundle:Default:login_register.html.twig') }}

<script src="{{ asset('bundles/foodapp/scripts/common.js')}}"></script>
