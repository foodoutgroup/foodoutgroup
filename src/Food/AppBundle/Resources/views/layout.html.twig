<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Welcome!{% endblock %}</title>



    <link rel="stylesheet" hrefs="{{ asset('css/bootstrap.css') }}">

    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="{{ asset('js/jquery.js') }}"><\/script>')</script>
    <script src="{{ asset('js/bootstrap.js') }}"></script>

    {% stylesheets
    '@FoodAppBundle/Resources/public/css_theme/*'
    filter="?yui_css" %}
    <link href="{{ asset_url }}" type="text/css" rel="stylesheet" media="screen" />
    {% endstylesheets %}

    {% stylesheets
    '@FoodAppBundle/Resources/public/css/*'
    filter="?yui_css" %}
    <link href="{{ asset_url }}" type="text/css" rel="stylesheet" media="screen" />
    {% endstylesheets %}

</head>
<body style="background-color: #f5f0eb;">
{% block menu %}
    {% include 'FoodAppBundle:Menu:default_menu_new.html.twig' %}
{% endblock %}
    <div class="container">
        <div class="row">
            {% block body %}{% endblock %}
            {{ render(controller('FoodDishesBundle:Kitchen:kitchenListWithImages', {})) }}
            {{ render(controller('FoodCartBundle:Default:sideBlock')) }}
        </div>
    </div>
{% block javascripts %}{% endblock %}
</body>
</html>