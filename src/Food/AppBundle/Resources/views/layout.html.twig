{{ notifications.init|raw }}
<div class="site">
    {% block delfi_banner %}{% endblock %}
    <div class="site-top">
        <div class="site-block clearfix">
            <a href="{{ path('food_lang_homepage') }}" class="main-logo">
                <img alt="{% trans %}general.base_page_title{% endtrans %} - {{ page_title }}"
                     src="{% if site_country == 'LT' %}{{ asset('bundles/foodapp/images/logo_food.png') }}{% elseif site_country == 'LV' %}{{ asset('bundles/foodapp/images/logo_food2.png') }}{% elseif site_country == 'EE' %}{{ asset('bundles/foodapp/images/logo_food_ee.png') }}{% endif %}"/>
            </a>

            {# <p class="contact-phone">1822 <span>užsakymas telefonu</span></p> #}
            <ul class="menu top-menu clearfix" data-toggle="popover" data-placement="bottom" data-content=""
                data-position-fix="840">
                <li><a href="{{ path('food_slug', {'slug': ('general.help_slug'|trans())}) }}"><span>{% trans %}
                            general.help_button{% endtrans %}</span></a></li>
            </ul>
            <ul class="menu top-menu clearfix" data-toggle="popover" data-placement="bottom" data-content=""
                data-position-fix="840">
                <li class="login-ico">
                    {% render(controller('FoodUserBundle:Default:loginButton')) %}
                </li>
            </ul>
            <div class="separator"></div>
            <div class="top-contact-block">
                {#
                {% if ("now"|date('d') == 31 or "now"|date('d') == 1) and site_country == 'LT' %}
                <span class="work-time" style="text-align: right">
                    12.31 10:00-18:00<br/>
                    01.01 12:00-21:00
                </span>
                {% elseif ("now"|date('d') == 24 or "now"|date('d') == 25) and site_country == 'LV' %}
                <span class="work-time" style="text-align: right">
                    12.24 11:00-00:00<br/>
                    12.25-26 11:00-23:00
                </span>
                {% else %}
                #}
                <span class="work-time">
                    <span>{% trans %}general.top_contact.worktime_days{% endtrans %}</span>
                    <span>{% trans %}general.top_contact.worktime{% endtrans %}</span>
                    {% if misc_utils.getparam('holdiday_working_time_active') == 1 %}
                        <br>
                        <span class="holiday-time" title="{{ misc_utils.getparam('holiday_working_time') }}">{% trans %}
                            general.top_contact.worktime_holiday{% endtrans %}</span>
                    {% endif %}
                </span>
                {#
                {% endif %}
                #}
                {% if site_country != 'EE' %}
                    <span class="phone"><a href="callto:{{ 'general.top_contact.phone'|trans()|replace({" " : ""}) }}"
                                           title="">{% trans %}general.top_contact.phone{% endtrans %}</a></span>
                {% endif %}
            </div>
        </div>
    </div>

    {% block body %}{% endblock %}
    <div class="site-center site-center-bottom">
        <div class="site-block">
            <div class="clearfix bottom-blocks">

                {% include 'FoodAppBundle:Default:bottom_block.html.twig' %}

                {% if show_apps_download %}
                    <div class="app-block clearfix"><img
                                src="//{{ cloudfront_url }}{{ asset('bundles/foodapp/images/app-view.png') }}">
                        <div class="app-block-inner">
                            <h3>{% trans %}general.app_block.header{% endtrans %}</h3>
                            <p>{% trans %}general.app_block.promo_text{% endtrans %}</p>
                            <div><a target="_BLANK" href="https://itunes.apple.com/us/app/foodout/id919992168"><img
                                            src="//{{ cloudfront_url }}{{ asset('bundles/foodapp/images/app-store.png') }}"></a><a
                                        target="_BLANK"
                                        href="https://play.google.com/store/apps/details?id=co.lemonlabs.android.foodout#"><img
                                            src="//{{ cloudfront_url }}{{ asset('bundles/foodapp/images/g-store.png') }}"></a>
                            </div>
                        </div>
                    </div>
                {% endif %}
            </div>
        </div>
    </div>

    {{ render(controller('FoodAppBundle:Default:footer')) }}
</div>

