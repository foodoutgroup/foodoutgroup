<!DOCTYPE html>
<html lang="{{ locale }}">
<head>
    <meta charset="utf-8">
    <meta name="verify-webtopay" content="461551ad8deddc3ad0975adb27d1eff4" />
    <title>{% block title %}{{ page_title }}{% endblock %}</title>
    <link rel="shortcut icon" href="/favicon.ico" />
    <link rel="apple-touch-icon" sizes="57x57" href="/apple-touch-icon-57x57.png">
    <link rel="apple-touch-icon" sizes="114x114" href="/apple-touch-icon-114x114.png">
    <link rel="apple-touch-icon" sizes="72x72" href="/apple-touch-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="144x144" href="/apple-touch-icon-144x144.png">
    <link rel="apple-touch-icon" sizes="60x60" href="/apple-touch-icon-60x60.png">
    <link rel="apple-touch-icon" sizes="120x120" href="/apple-touch-icon-120x120.png">
    <link rel="apple-touch-icon" sizes="76x76" href="/apple-touch-icon-76x76.png">
    <link rel="apple-touch-icon" sizes="152x152" href="/apple-touch-icon-152x152.png">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="/mstile-144x144.png">
    <meta name="description" content="">
    <meta name="viewport" content="width=1000">
    <meta name="msapplication-config" content="none"/>

    {% stylesheets 'bundles/foodapp/scripts/jquery-ui/custom-theme/jquery-ui-1.10.3.custom.min.css' filter="cssrewrite" %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css"  />
    {% endstylesheets %}
    {% stylesheets '@FoodAppBundle/Resources/public/scripts/boxer/jquery.fs.boxer.css*'  %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css"  />
    {% endstylesheets %}
    {% stylesheets '@FoodAppBundle/Resources/public/styles/style.css'  %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css"  />
    {% endstylesheets %}
    {% stylesheets '@FoodAppBundle/Resources/public/styles/jquery.loadmask.css'  %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css"  />
    {% endstylesheets %}

    {% block javascripts %}
    <script src="{{ asset('bundles/foodapp/scripts/jquery-1.10.2.min.js') }}"></script>
    <script src="{{ asset('bundles/foodapp/scripts/jquery.cycle2.min.js') }}"></script>
    <script src="{{ asset('bundles/foodapp/scripts/jquery-ui/jquery-ui-1.10.3.custom.min.js') }}"></script>
    <script src="{{ asset('bundles/foodapp/scripts/jquery.ui.selectmenu.js') }}"></script>
    <script src="{{ asset('bundles/foodapp/scripts/jquery.raty.js') }}"></script>
    <script src="{{ asset('bundles/foodapp/scripts/boxer/jquery.fs.boxer.min.js') }}"></script>
    <script src="{{ asset('bundles/foodapp/scripts/icheck/jquery.icheck.min.js') }}">		</script>
    <script src="{{ asset('bundles/foodapp/scripts/jquery.placeholder.min.js') }}"></script>
    <script src="{{ asset('bundles/foodapp/scripts/jquery.loadmask.min.js') }}"></script>
    <script src="{{ asset('bundles/foodapp/scripts/jquery.typeahead.js') }}"></script>
    {# <script src="{{ asset('bundles/foodapp/scripts/mediator.min.js') }}"></script> #}
    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
    <!--[if IE 8]><script src="{{ asset('bundles/foodapp/scripts/selectivizr-min.js') }}"></script><![endif]-->
    <!--[if IE 9]><script src="{{ asset('bundles/foodapp/scripts/pie_ie9.js') }}"></script><![endif]-->
    <!--[if IE 8]><script src="{{ asset('bundles/foodapp/scripts/pie_ie8.js') }}"></script><![endif]-->
    <script type="text/javascript">
        $.fn.raty.defaults.noRatedMsg = '{% trans %}general.raty.not_rated_yet{% endtrans %}';
        $.fn.raty.defaults.hints = [
            '{% trans %}general.raty.bad{% endtrans %}',
            '{% trans %}general.raty.poor{% endtrans %}',
            '{% trans %}general.raty.regular{% endtrans %}',
            '{% trans %}general.raty.good{% endtrans %}',
            '{% trans %}general.raty.gorgeous{% endtrans %}'
        ];
    </script>
    <script src="{{ asset('bundles/foodapp/scripts/noty/packaged/jquery.noty.packaged.min.js') }}"></script>
    <script src="{{ asset('bundles/foodapp/scripts/bootstrap/js/bootstrap.min.js') }}"></script>
    {% endblock %}
</head>
<body>
{{ notifications.init|raw }}
<div class="site">
    <div class="site-top">
        <div class="site-block clearfix"><a href="{{ path('food_lang_homepage') }}" class="main-logo"><img alt="{{ page_title }}" src="{{ asset('bundles/foodapp/images/logo_food.png') }}"></a>
            {# <p class="contact-phone">1822 <span>u≈æsakymas telefonu</span></p> #}
            <ul class="menu top-menu clearfix" data-toggle="popover" data-placement="bottom" data-content="" data-position-fix="840">
                <li><a href="{{ path('food_slug', {'slug': ('general.help_slug'|trans())}) }}"><span>{% trans %}general.help_button{% endtrans %}</span></a></li>
                <li class="login-ico">
                    {% render(controller('FoodUserBundle:Default:loginButton')) %}
                </li>
            </ul>
        </div>
    </div>

    {% block body %}{% endblock %}

    {{ render(controller('FoodAppBundle:Default:footer')) }}
</div>

{{ include('FoodAppBundle:Default:login_register.html.twig') }}

<script src="{{ asset('bundles/foodapp/scripts/common.js')}}"></script>
<script src="{{ asset('bundles/foodapp/scripts/scripts.js')}}"></script>
<script type="text/javascript">
    {# var mediator = new Mediator(); #}
    {% block jscode %}{% endblock %}
</script>
<script>
