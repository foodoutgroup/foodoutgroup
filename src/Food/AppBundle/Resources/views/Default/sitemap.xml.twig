<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc>http://{{ domain }}{{ slug.toHomepage() }}</loc>
        <changefreq>hourly</changefreq>
    </url>
    {#<url>#}
        {#<loc>http://{{ domain }}{{ path('food_places_lt_recommended', {}, false) }}</loc>#}
        {#<changefreq>hourly</changefreq>#}
    {#</url>#}
    {#{% for locale in availableLocales %}#}
    {#{% for place in places %}#}
    {#<url>#}
        {#<loc>http://{{ domain }}{{ path('food_slug', {'slug': slug_util.slugByItem(place.id, 'place')}) }}</loc>#}
        {#<changefreq>daily</changefreq>#}
    {#</url>#}
    {#{% endfor %}#}
    {% for city in cities %}
        <url>
            <loc>http://{{ domain }}{{ slug.getUrl(city.id, 'city') }}</loc>
            <changefreq>daily</changefreq>
        </url>
    {% endfor %}
    {% for cityId, kitchen in cityKitchenCollection %}

        {% for kitchen in city %}
            <url>
                <loc>http://{{ domain }}{{ slug.path(cityId, 'city') }}/{{ slug.path(kitchen.id, 'kitchen') }}</loc>
                <changefreq>daily</changefreq>
            </url>
        {% endfor %}
    {% endfor %}
</urlset>