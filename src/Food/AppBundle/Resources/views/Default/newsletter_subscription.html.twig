{# Padeka, po registracijos i newsletteri #}
{% extends 'FoodAppBundle::layout.html.twig' %}

{% block title %}{% trans %}general.newsletter{% endtrans %} | {{ page_title }}{% endblock %}

{% block adnet_retargeting %}
    <!-- Adform Tracking Code BEGIN -->
    <script type="text/javascript">
        var _adftrack = {
            pm: 314127,
            divider: encodeURIComponent('|'),
            pagename: encodeURIComponent('Foodout.lt|{% trans %}general.homepage.meta_title{% endtrans %}|{% trans %}general.newsletter{% endtrans %}')
        };
        (function () { var s = document.createElement('script'); s.type = 'text/javascript'; s.async = true; s.src = 'https://track.adform.net/serving/scripts/trackpoint/async/'; var x = document.getElementsByTagName('script')[0]; x.parentNode.insertBefore(s, x); })();

    </script>
    <noscript>
        <p style="margin:0;padding:0;border:0;">
            <img src="https://track.adform.net/Serving/TrackPoint/?pm=314127&ADFPageName=Foodout.lt|{% trans %}general.homepage.meta_title{% endtrans %}|{% trans %}general.newsletter{% endtrans %}&ADFdivider=|" width="1" height="1" alt="" />
        </p>
    </noscript>
    <!-- Adform Tracking Code END -->
{% endblock %}

    {% block body %}
        {# Facebook Lead Begin #}
        <script>fbq('track', 'Lead');</script>
        {# Facebook Lead End #}
        <div class="site-center">
            <div class="site-block">
                <div class="middle-content">
                    <div class="static-page">
                        <h1>{% trans %}general.thank_you{% endtrans %}</h1>
                        {% trans %}general.subscribed_newsletter{% endtrans %}
                    </div>
                </div>
            </div>
        </div>
    {% endblock %}
