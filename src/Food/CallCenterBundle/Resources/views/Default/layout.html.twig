{% extends 'FoodAppBundle::mobilelayout.html.twig' %}

{% block title %}{{ 'callcenter.title'|trans}} - {{ page_title }}{% endblock %}
{% block head_javascripts %}
    {{ parent() }}

    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key={{ google_maps_api }}&sensor=false"></script>

    <script type="text/javascript" src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script type="text/javascript" src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>

    <script type="text/javascript" src="{{ asset('bundles/foodapp/scripts/select2/select2.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/foodapp/scripts/select2/select2_locale_lt.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/foodapp/scripts/jquery.cycle2.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/foodapp/scripts/jquery-ui/jquery-ui-1.10.3.custom.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/foodapp/scripts/jquery.ui.selectmenu.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/foodapp/scripts/jquery.raty.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/foodapp/scripts/boxer/jquery.fs.boxer.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/foodapp/scripts/jquery.placeholder.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/foodapp/scripts/jquery.loadmask.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/foodapp/scripts/jquery.typeahead.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/foodapp/scripts/fancy/jquery.fancybox.pack.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/foodapp/scripts/fancy/helpers/jquery.fancybox-media.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/foodapp/scripts/isloading/jquery.isloading.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/foodapp/scripts/common.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/foodapp/scripts/scripts.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/foodapp/scripts/cart.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/foodapp/js_main/place.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/callcenter/js/callcenter.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/foodapp/js_main/order.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/foodapp/scripts/checkout.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/foodapp/scripts/icheck/jquery.icheck.min.js') }}"></script>
    {# Chosen JS Begin #}
    {% javascripts '@FoodAppBundle/Resources/public/scripts/chosen/chosen.jquery.min.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    {# Chosen JS End #}

    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

        ga('create', 'UA-50355577-1', 'foodout.lt');
        ga('require', 'displayfeatures');
        ga('send', 'pageview');
        ga('require', 'ec');
    </script>
    {# Facebook Pixel Code Begin #}
    <script>
        !function(f,b,e,v,n,t,s){if(f.fbq)return;n=f.fbq=function(){n.callMethod? n.callMethod.apply(n,arguments):n.queue.push(arguments)};if(!f._fbq)f._fbq=n; n.push=n;n.loaded=!0;n.version='2.0';n.queue=[];t=b.createElement(e);t.async=!0; t.src=v;s=b.getElementsByTagName(e)[0];s.parentNode.insertBefore(t,s)}(window,document,'script','https://connect.facebook.net/en_US/fbevents.js');
