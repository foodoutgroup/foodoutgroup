{% endblock %}
{% block stylesheets %}
    {{ parent() }}
    {% stylesheets 'bundles/foodapp/styles/preload.css'
                   'bundles/foodapp/scripts/select2/select2.css'
                   'bundles/foodapp/scripts/select2/select2-bootstrap.css'
                   'bundles/foodapp/styles/jquery.loadmask.css'
                   'bundles/foodapp/scripts/jquery-ui/custom-theme/jquery-ui-1.10.3.custom.min.css'
                   'bundles/foodapp/scripts/boxer/jquery.fs.boxer.css'
                   'bundles/foodapp/scripts/font-awesome-4.2.0/css/font-awesome.min.css'
                   'bundles/foodapp/scripts/fancy/jquery.fancybox.css'
                   'bundles/foodapp/styles/style.css'
                   'bundles/foodapp/styles/callcenter.css'
                   filter="cssrewrite" %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css"  />
    {% endstylesheets %}

    {# Chosen CSS Begin #}
    {% stylesheets '@FoodAppBundle/Resources/public/scripts/chosen/chosen.min.css' filter="cssrewrite" %}
    <link rel="stylesheet" href="{{ asset_url }}" type="text/css"  />
    {% endstylesheets %}
    {# Chosen CSS End #}
{% endblock %}
{% block body %}
    <nav class="navbar navbar-default navbar-static-top" role="navigation">
        <div class="container-fluid">
            <div class="navbar-header">
                <a class="navbar-brand" href="">{{ 'callcenter.title'|trans }}</a>
            </div>
            <div class="collapse navbar-collapse pull-right" id="bs-example-navbar-collapse-1">
                <form class="navbar-form navbar-left" action="{{ path('fos_user_security_logout') }}" role="search">
                    <input type="text" value="370" id="top-phone-input" class="form-control" placeholder="3706000000" style="width: 150px;" />
                    <button type="button" id="top-phone-search" class="btn btn-default">Adresas pagal telefona</button>
                    &nbsp;&nbsp;
                    <button type="submit" class="btn btn-default">Atsijungti</button>
                </form>
            </div>
            <div class="clear-pull-right"></div>
        </div>
    </nav>
    {# preload images #}
    <div class="images-for-preload">
        <span class="fa fa-cog fa-spin"></span>
    </div>
{% endblock %}
