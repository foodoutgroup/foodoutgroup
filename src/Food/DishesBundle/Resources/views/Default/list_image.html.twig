{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}

{% block field%}
    {%  if object.imageSetted !=null %}
        {% if object.multipleThumbs %}
            <img src="/{{object.getWebPathThumb('type2')}}">
        {% else %}
            <img src="/{{object.getWebPathThumb}}">
        {% endif %}
    {% endif %}
{% endblock %}